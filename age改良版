int main()
{
	int age= 17;
	scanf("%d",&age);
	if (age < 18)
	{
		printf("未成年\n");
		printf("不能谈恋爱\n");
	}

	else
	{
		if (age >= 18 && age < 28)
			printf("青年\n");
		else if (age >= 28 && age < 60)
			printf("壮年\n");
		else if (age >= 60 && age < 100)
			printf("老年\n");
		else
			printf("百岁老人\n");
	}
	return 0;
}
